# Vercel AI Gateway Configuration
VERCEL_GATEWAY_API_KEY=your_api_key_here
VERCEL_GATEWAY_URL=https://ai-gateway.vercel.sh/v1/ai

# Model Selection
VERCEL_GATEWAY_DEFAULT_MODEL=openai/gpt-4o-mini
VERCEL_GATEWAY_LARGE_MODEL=openai/gpt-5
VERCEL_GATEWAY_EMBEDDING_MODEL=openai/text-embedding-3-small

# Cache and Retry
VERCEL_GATEWAY_CACHE_TTL=300
VERCEL_GATEWAY_MAX_RETRIES=3

# OIDC (for Vercel deployments)
VERCEL_GATEWAY_USE_OIDC=false
